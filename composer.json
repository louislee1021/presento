{
    "name": "louis1021/presento",
    "description": "Presento is a package for PHP 5.6.40 data presenter",
    "type": "library",
    "require": {
        "php": ">=5.6.40",
        "ext-json": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "3.0.0",
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Louis Lee",
            "email": "louis@at-creative.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "helpers/presento.php"
        ],
        "psr-4": {
            "Louis1021\\Presento\\": "src/",
            "Example\\": "example/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Louis1021\\Presento\\Tests\\": "tests/"
        }
    }
}
